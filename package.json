{
   "name": "mister-proper",
   "version": "1.0.0",
   "description": "Api for telegram bot",
   "engines": {
      "node": "10.16.0",
      "npm": "6.x"
    },
   "devDependencies": {
      "@types/jest": "^24.0.13",
      "@types/jsonwebtoken": "^8.3.2",
      "@types/node": "^8.0.29",
      "@typescript-eslint/eslint-plugin": "^1.9.0",
      "@typescript-eslint/parser": "^1.9.0",
      "axios-logger": "^2.1.0",
      "eslint": "^5.16.0",
      "jest": "^24.8.0",
      "supertest": "^4.0.2",
      "swagger-jsdoc": "^3.2.9",
      "swagger-ui-express": "^4.0.6",
      "ts-jest": "^24.0.2",
      "ts-node": "3.3.0",
      "ts-node-dev": "^1.0.0-pre.39",
      "typescript": "3.3.3333"
   },
   "dependencies": {
      "@types/express": "^4.17.1",
      "@types/node-telegram-bot-api": "^0.31.0",
      "axios": "^0.19.0",
      "axios-debug-log": "^0.6.2",
      "bcryptjs": "^2.4.3",
      "body-parser": "^1.18.1",
      "class-validator": "^0.9.1",
      "colors": "^1.3.3",
      "cors": "^2.8.5",
      "cross-env": "^5.2.0",
      "deepmerge": "^4.0.0",
      "express": "^4.15.4",
      "express-logger": "0.0.3",
      "helmet": "^3.18.0",
      "http": "0.0.0",
      "jsonwebtoken": "^8.5.1",
      "morgan": "^1.9.1",
      "ngrok": "^3.2.5",
      "node-telegram-bot-api": "^0.30.0",
      "node-telegram-bot-api-middleware": "^0.1.3",
      "pg": "^7.11.0",
      "reflect-metadata": "^0.1.10",
      "sqlstring": "^2.3.1",
      "telegraf": "^3.32.0",
      "typeorm": "0.2.17"
   },
   "scripts": {
      "start": "set debug && ts-node-dev --respawn --transpileOnly ./src/server",
      "set-webhook": "./scripts/set-webhook.sh",
      "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
      "debug": "cross-env TZ=UTC NODE_ENV=development tsnd --inspect --respawn ./src/server",
      "pretest": "eslint src/**/*.ts",
      "test": "cross-env NODE_ENV=test jest",
      "build": "tsc",
      "swagger:generate": "./node_modules/swagger-jsdoc/bin/swagger-jsdoc.js -d swaggerDef.js src/controllers/*.ts"
   },
   "jest": {
      "transform": {
         "^.+\\.tsx?$": "ts-jest"
      }
   }
}
